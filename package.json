{
  "name": "bible-exchange",
  "version": "1.0.0",
  "description": "Bible exchange app",
  "scripts": {
    "start": "webpack-dev-server -d --hot --inline --no-info --port 3000",
    "build": "webpack -d --inline --history-api-fallback --config webpack.config.js",
    "relay": "relay-compiler --src ./src --schema ./server/data/schema.graphql --watch",
    "update": "babel-node server/index.js",
    "deploy": "NODE_ENV=production webpack --config webpack.production.config.js && NODE_ENV=production babel-node server/index.js",
    "lint": "eslint --fix src",
    "build:webpack": "NODE_ENV=production webpack --progress --colors --config webpack.production.config.js",
    "update-schema": "babel-node ./server/utils/updateSchema.js"
  },
  "pre-commit": "lint",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bibleexchange/bibleexchange.git"
  },
  "keywords": [
    "bible"
  ],
  "author": "Varayut Lerdkanlayanawat",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bibleexchange/bibleexchange/issues"
  },
  "homepage": "https://github.com/bibleexchange/bibleexchange",
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-cli": "^6.8.0",
    "babel-core": "^6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-relay": "dev",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-relay-plugin": "^0.8.1",
    "body-parser": "^1.15.2",
    "chalk": "^1.1.3",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.1",
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-import": "^1.8.0",
    "eslint-plugin-jsx-a11y": "^1.2.0",
    "eslint-plugin-react": "^5.1.1",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.16.1",
    "json-loader": "^0.5.4",
    "lodash": "^4.12.0",
    "nodemon": "^1.11.0",
    "postcss-loader": "^0.9.1",
    "pre-commit": "^1.1.3",
    "precss": "^1.4.0",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.3.6",
    "relay-compiler": "dev",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "jscodeshift": "^0.3.32",
    "marked": "^0.3.6",
    "react": "^15.5.4",
    "react-addons-css-transition-group": "^15.3.0",
    "react-addons-update": "^15.6.0",
    "react-dom": "^15.5.4",
    "react-relay": "dev",
    "react-router": "^2.8.1",
    "react-router-relay": "^0.13.7",
    "react-transition-group": "^1.1.3",
    "soundcloud": "^3.1.2",
    "watchman": "dev"
  }
}
